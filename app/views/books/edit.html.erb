<h2>Editing Book</h2>

<div class="book">
  <%= form_with model: @book do |f| %>
    <h5>Title</h5>
    <%= f.text_field :title %>
    <h5>Body</h5>
    <%= f.text_area :impression %>

    <div class="book-post">
      <%= f.submit 'Update Book' %>
    </div>
  <% end %>
</div>

<table class="book-column">
  <thead>
    <th></th>
    <th></th>
  </thead>
  <tbody>
    <tr>
      <td class="border"><%= link_to "Show", book_path(@book.id) %></td>
      <td><%= link_to "Beak", books_path(@book.id) %></td>
    <tr>
  <tbody>

